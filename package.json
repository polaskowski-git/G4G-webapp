{
  "name": "sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=prod node build/index.js",
    "dev": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "dev:watch": "nodemon --config nodemon.json --exec npm run dev ",
    "build": "rimraf build && rimraf public/build && tsc ",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js --config ./src/constants/typeorm.cli.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:show": "npm run typeorm migration:show",
    "migration:generate": "npm run typeorm migration:generate -- -n 'AutoGenerated'"
  },
  "author": "Andrzejewski Kamil",
  "license": "ISC",
  "dependencies": {
    "class-validator": "^0.12.2",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "i18n": "^0.10.0",
    "ini": "^1.3.5",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "lodash": "^4.17.15",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "twig": "^1.15.1",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.13",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
